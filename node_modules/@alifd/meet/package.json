  "scripts": {
    "start": "node scripts/start.js",
    "start:demos": "cd demos && tnpm i && npm run build",
    "precommit": "lint-staged",
    "theme": "theme-scripts start --env=mobile",
    "theme:build": "theme-scripts build --env=mobile",
    "theme:publish": "theme-scripts publish --name=meet",
    "build": "build-scripts build",
    "build:docs": "build-scripts build --config build.doc.json && theme-scripts build --env=mobile",
    "build:demos": "rm -rf demos && node scripts/demos/demo.js",
    "herbox:daily": "node scripts/demos/herbox.js",
    "herbox": "npm run build:demos && node scripts/demos/herbox.js --prod",
    "release": "node scripts/release.js",
    "lint": "eslint --fix '@(docs|scripts|src)/**/*.@(js|jsx)' && tslint --fix '@(src|test)/**/*.@(ts|tsx)'",
    "lint:nofix": "eslint '@(docs|scripts|src)/**/*.@(js|jsx)' && tslint '@(src|test)/**/*.@(ts|tsx)'  ",
    "compile:react": "cd scripts/rax2react && gulp",
    "dist:react": "webpack --mode production --config scripts/rax2react/webpack.config.js",
    "build:react": "npm run compile:react && npm run dist:react",
    "release:react": "npm run build:react && node scripts/rax2react/release-react.js",
    "dev:react": "node scripts/rax2react/devServer.js",
    "dev-demos:react": "webpack-dev-server --mode development --config scripts/rax2react/webpack.demo.config.js",
    "test": "node ./scripts/test.js",
    "coverage": "nyc --reporter=html node ./scripts/test.js"
  },
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git@gitlab.alibaba-inc.com:fusion-mobile/meet.git"
